#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Rus");
	cout << "\n\n\n";
	cout << "\t\t\t\tСовременный программист\n\n";
	cout << "\t\t\t\tЯ программист, осознавший едва\n";
	cout << "\t\t\t\tВсю суть программистских контор.\n";
	cout << "\t\t\t\tЯ миддл опять в мои двадцать два,\n";
	cout << "\t\t\t\tА в двадцать один был сеньор.\n\n";
	cout << "\t\t\t\tРабота идет, и, как ни крути,\n";
	cout << "\t\t\t\tМеня в ремесло посвятят.\n";
	cout << "\t\t\t\tНа джуна я выйду годам к тридцати,\n";
	cout << "\t\t\t\tА в сорок пойду в детский сад. \n";
	cout << "\n\n\n";
	cout << "\n\n\n";
}
